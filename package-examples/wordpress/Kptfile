apiVersion: kpt.dev/v1alpha2
kind: Kptfile
metadata:
  name: wordpress
info:
  description: This is an example wordpress package with mysql subpackage
  emails:
  - kpt-team@google.com
pipeline:
  mutators:
  - image: gcr.io/kpt-fn/apply-setters:v0.1
    configMap:
      wp-image: wordpress
      wp-tag: 4.8-apache
