  shortDescription: kpt example
pipeline:
  mutators:
    - image: gcr.io/kpt-fn/apply-setters:unstable
      configMap:
        replicas: 5
        image: gcr.io/kpt-dev/helloworld-gke
        tag: latest
        http-port: 80