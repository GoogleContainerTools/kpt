apiVersion: kpt.dev/v1alpha2
kind: Kptfile
metadata:
  name: kpt-pipeline-validate-example
pipeline:
  mutators:
    - image: gcr.io/kpt-functions/label-namespace
      configMap:
        label_name: color
        label_value: blue      
  validators:
    - image: gcr.io/kpt-functions/gatekeeper-validate
